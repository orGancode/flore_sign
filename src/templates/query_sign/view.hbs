<div class="query-sign-view">
  {{> '../../partials/head/view.hbs'}}
  <form class="body form query-sign-form" id='query-sign-form'>
    <h1>查询报名信息<span>(备注：带<strong>*</strong>为必填项)</span></h1>
    <div class="input-group">
      <label for=""><span class="required">*</span>姓名：</label>
      <div>
        <input type="text" name="name" maxlength="20" required>
      </div>
    </div>
    <div class="input-group">
      <label for=""><span class="required">*</span>手机号：</label>
      <div>
        <input type="text" name="phone" required>
      </div>
    </div>
    <div class="input-group">
      <label for=""><span class="required">*</span>课程：</label>
      <div>
        <select name="course_id" id="">
          {{#each DATA}}
            <option value="{{id}}">{{name}}</option>
          {{/each}}
        </select>
      </div>
    </div>
    <div class="btn-group">
      <input class="btn btn-primary" type="submit" value="查询">
    </div>
  </form>
</div>
